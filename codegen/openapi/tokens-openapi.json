        "parameters": [
          {
            "name": "chain_id",
            "required": true,
            "in": "path",
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "provider",
            "required": false,
            "in": "query",
            "description": "Provider code. Default value is \"1inch\"",
            "example": "1inch",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "country",
            "required": false,
            "in": "query",
            "description": "Country code",
            "example": "US",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Token list",
