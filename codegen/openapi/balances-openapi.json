                    "properties": {
                      "balance": {
                        "type": "string"
                      },
                      "allowance": {
                        "type": "string"
                      }
                    }
                  },
                  "example": {
                    "0x0327112423f3a68efdf1fcf402f6c5cb9f7c33fd": {
                      "balance": "0",
